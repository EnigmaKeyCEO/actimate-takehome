{
  "cli": {
    "version": ">= 7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.18.2",
      "yarn": "1.22.22",
      "env": {},
      "android": {
        "image": "default",
        "env": {
          "PLATFORM": "android",
          "IS_WEB": "false"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "PLATFORM": "ios",
          "IS_WEB": "false"
        }
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "env": {
        "NODE_ENV": "development"
      },
      "android": {
        "distribution": "internal",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": false
      }
    },
    "development-simulator": {
      "extends": "development",
      "env": {
        "NODE_ENV": "development"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "extends": "base",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "preview": {},
    "production": {}
  }
}
